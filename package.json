{
  "name": "n8n-nodes-phorge",
  "version": "2.0.0",
  "description": "n8n nodes for Phorge",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Emil Haugbergsmyr",
    "email": "rrvenn@proton.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tagnard/n8n-nodes-phorge.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/PhorgeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Phorge/Phorge.node.js",
      "dist/nodes/PhorgeTrigger/PhorgeTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.2.3",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.120.6"
  },
  "dependencies": {
    "phorge-ts": "^1.0.1"
  }
}
